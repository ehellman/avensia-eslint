{
  "name": "eslint-plugin-avensia",
  "version": "0.0.0",
  "author": "Erik Hellman <erik.hellman@avensia.com>",
  "description": "Avensia's suite of custom ESLint rules",
  "main": "index.js",
  "scripts": {
    "prepack": "npmignore --auto --commentLines=autogenerated",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "pretest": "npm run lint",
    "test": "npm run unit-test",
    "unit-test": "istanbul cover node_modules/mocha/bin/_mocha tests/lib/**/*.js tests/index.js",
    "generate-list-of-rules": "md-magic --path README.md",
    "generate-list-of-rules:check": "npm run generate-list-of-rules && git diff --exit-code README.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/avensia-oss/eslint"
  },
  "homepage": "https://github.com/avensia-oss/eslint",
  "bugs": "https://github.com/avensia-oss/eslint/issues",
  "devDependencies": {
    "eslint": "^3 || ^4 || ^5 || ^6 || ^7 || ^8",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-plugin": "^2.3.0 || ^3.5.3 || ^4.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-scope": "^3.7.3",
    "espree": "^3.5.4",
    "istanbul": "^0.4.5",
    "ls-engines": "^0.6.6",
    "markdown-magic": "^2.6.0",
    "mocha": "^5.2.0",
    "npmignore": "^0.3.0",
    "prettier": "^2.6.2"
  },
  "peerDependencies": {
    "eslint": "^3 || ^4 || ^5 || ^6 || ^7 || ^8"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "eslint",
    "eslint-plugin",
    "eslintplugin",
    "react"
  ],
  "license": "MIT",
  "publishConfig": {
    "ignore": [
      ".github/",
      "!lib",
      "docs/",
      "test/",
      "tests/",
      "*.md",
      "*.config.js",
      ".eslintrc",
      ".editorconfig",
      "tsconfig.json"
    ]
  }
}
